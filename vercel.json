{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/server.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11",
    "VERCEL": "1"
  },
  "functions": {
    "frontend/server.py": {
      "runtime": "python3.11",
      "maxDuration": 30,
      "memory": 2048
    }
  }
}
